<div id='asset_info'>
  <h2>{{ asset.name }}</h2>
</div>
<div id='asset_related_data_tab_container'>
</div>

<script type='text/javascript'>
  require(['dijit/registry', 'dijit/layout/TabContainer', 'dojox/layout/ContentPane', 'dojo/domReady!'],
          function(registry, TabContainer, ContentPane){

            alert('This is running 188');
            // **********************************************************************
            // Bottom Part - Complex Data View
            var asset_related_data_tab_container = new TabContainer({
              region: 'bottom'
            }, 'asset_related_data_tab_container');

            var asset_summary_content_pane = new ContentPane({
              title: 'Summary',
              href: 'summarize/asset/{{ asset.id }}',
              selected: true
            });

            var asset_users_content_pane = new ContentPane({
              title: 'Users',
              href: 'view/users/{{ asset.id }}'
            });

            var asset_tasks_content_pane = new ContentPane({
              title: 'Tasks',
              href: 'view/tasks/{{ asset.id }}'
            });

            var asset_tickets_content_pane = new ContentPane({
              title: 'Tickets',
              content: 'view/tickets/{{ asset.id }}'
            });

            var asset_versions_content_pane = new ContentPane({
              title: 'Versions',
              href: 'view/versions/{{ asset.id }}'
            });

            asset_related_data_tab_container.addChild(asset_summary_content_pane);
            asset_related_data_tab_container.addChild(asset_users_content_pane);
            asset_related_data_tab_container.addChild(asset_tasks_content_pane);
            asset_related_data_tab_container.addChild(asset_tickets_content_pane);
            asset_related_data_tab_container.addChild(asset_versions_content_pane);
            asset_related_data_tab_container.startup();


          });
</script>